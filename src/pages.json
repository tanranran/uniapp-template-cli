{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "unibest",
    "navigationBarBackgroundColor": "#F8F8F8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "subAsyncEcharts",
        "subEcharts"
      ]
    }
  },
  "pages": [
    // #ifdef H5 || MP-WEIXIN
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "我是首页"
      },
      "name": "PAGES_INDEX_INDEX"
    },
    // #endif
    {
      "path": "pages/index/paging",
      "type": "page",
      "name": "PAGES_INDEX_PAGING"
    },
    {
      "path": "pages/index/testA",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testA"
      },
      "name": "PAGES_INDEX_TESTA"
    },
    {
      "path": "pages/index/testB",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testB"
      },
      "name": "PAGES_INDEX_TESTB"
    }
  ],
  "subPackages": [
    {
      "root": "subPages",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: H5 || MP-WEIXIN
        {
          "path": "styles/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "UnoCSS 演示"
          },
          "name": "STYLES_INDEX"
        }
      ]
    },
    {
      "root": "subEcharts",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: H5 || MP-WEIXIN
        {
          "path": "echarts/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "Echarts 演示"
          },
          "name": "ECHARTS_INDEX"
        }
      ]
    },
    {
      "root": "subAsyncEcharts",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: H5 || MP-WEIXIN
        {
          "path": "asyncEcharts/index",
          "type": "page",
          "name": "ASYNCECHARTS_INDEX",
          "style": {
            "navigationBarTitleText": "echarts 异步跨包示例"
          }
        }
      ]
    }
  ]
}
