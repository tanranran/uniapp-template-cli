{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "uniapp-template-cli",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": []
    }
  },
  "pages": [
    // #ifdef APP || MP-HARMONY
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "我是首页"
      },
      "name": "PAGES_INDEX_INDEX"
    },
    // #endif
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "我是首页",
        "enablePullDownRefresh": true
      },
      "name": "PAGES_INDEX_INDEX"
    },
    {
      "path": "pages/index/paging",
      "type": "page",
      "name": "PAGES_INDEX_PAGING"
    },
    {
      "path": "pages/index/testA",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testA"
      },
      "name": "PAGES_INDEX_TESTA"
    },
    {
      "path": "pages/index/testB",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testB"
      },
      "name": "PAGES_INDEX_TESTB"
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: APP || H5 || MP-HARMONY || MP-WEIXIN
        {
          "path": "styles/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "UnoCSS 演示"
          },
          "name": "STYLES_INDEX"
        }
      ]
    }
  ]
}
