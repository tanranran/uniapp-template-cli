{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "uniapp-template-cli",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "pages-sub"
      ]
    }
  },
  "pages": [
    // GENERATED BY UNI-PAGES, PLATFORM: H5 || MP-WEIXIN
    {
      "path": "pages/index/paging",
      "type": "home",
      "style": {
        "navigationBarTitleText": "我是首页"
      },
      "needLogin": true,
      "name": "PAGES_INDEX_PAGING"
    },
    // #ifdef MP-WEIXIN
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "日历组件测试"
      },
      "name": "PAGES_INDEX_INDEX"
    },
    // #endif
    // #ifdef H5
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页"
      },
      "name": "PAGES_INDEX_INDEX"
    },
    // #endif
    {
      "path": "pages/index/testA",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testA"
      },
      "name": "PAGES_INDEX_TESTA"
    },
    {
      "path": "pages/index/testB",
      "type": "page",
      "style": {
        "navigationBarTitleText": "我是testB"
      },
      "name": "PAGES_INDEX_TESTB"
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        // GENERATED BY UNI-PAGES, PLATFORM: H5 || MP-WEIXIN
        {
          "path": "404/index",
          "type": "page",
          "style": {
            "navigationStyle": "custom"
          },
          "name": "404_INDEX"
        },
        {
          "path": "login/login",
          "type": "page",
          "style": {
            "navigationBarTitleText": "登录"
          },
          "name": "LOGIN_LOGIN"
        },
        {
          "path": "styles/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "UnoCSS 演示"
          },
          "name": "STYLES_INDEX"
        },
        // #ifdef MP-WEIXIN
        {
          "path": "WebView",
          "type": "page",
          "style": {
            "navigationBarTitleText": "..."
          },
          "name": "WEBVIEW_WEBVIEW"
        },
        // #endif
        // #ifdef H5
        {
          "path": "webview/webView",
          "type": "page",
          "style": {
            "navigationBarTitleText": "..."
          },
          "name": "WEBVIEW_WEBVIEW"
        }
        // #endif
      ]
    }
  ]
}
